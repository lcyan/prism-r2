# Development Environment Variables
# Copy this file to .env.local and fill in your R2 credentials for local testing

VITE_DEV_R2_ACCOUNT_ID=your_account_id
VITE_DEV_R2_ACCESS_KEY_ID=your_access_key_id
VITE_DEV_R2_SECRET_ACCESS_KEY=your_secret_access_key
VITE_DEV_R2_BUCKET_NAME=your_bucket_name
VITE_DEV_R2_CUSTOM_DOMAIN=https://your-domain.com
VITE_DEV_R2_ENDPOINT=https://your_account_id.r2.cloudflarestorage.com
